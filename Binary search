#include <stdio.h>
#include <string.h>

int main() {
    char names[10][50], key[50];
    int n, low, high, mid, found = 0;

    printf("Enter number of names: ");
    scanf("%d", &n);
    printf("Enter names in sorted order:\n");
    for (int i = 0; i < n; i++)
        scanf("%s", names[i]);

    printf("Enter name to search: ");
    scanf("%s", key);

    low = 0;
    high = n - 1;

    while (low <= high) {
        mid = (low + high) / 2;
        int cmp = strcmp(key, names[mid]);
        if (cmp == 0) {
            printf("Name found at position %d\n", mid + 1);
            found = 1;
            break;
        } else if (cmp < 0)
            high = mid - 1;
        else
            low = mid + 1;
    }

    if (!found)
        printf("Name not found.\n");
    return 0;
}
