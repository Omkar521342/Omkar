#include <stdio.h>
#include <string.h>

int main() {
    char names[10][50], key[50];
    int n, found = 0;

    printf("Enter number of names: ");
    scanf("%d", &n);
    printf("Enter names:\n");
    for (int i = 0; i < n; i++)
        scanf("%s", names[i]);

    printf("Enter name to search: ");
    scanf("%s", key);

    for (int i = 0; i < n; i++) {
        if (strcmp(names[i], key) == 0) {
            printf("Name found at position %d\n", i + 1);
            found = 1;
            break;
        }
    }

    if (!found)
        printf("Name not found.\n");
    return 0;
}
